<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.douzi.demo"
    android:versionCode="1"
    android:versionName="1.0" >
    
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".TextActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".EditTextActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".CheckBoxActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".RadioGroupActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".IntentActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".IntentToActivity"
            android:label="@string/app_name" >
            <intent-filter >
                <action android:name="com.douzi.demo.INTENTTO" />
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:scheme="http"/>
            </intent-filter>
        </activity>
         <activity
            android:name=".ActivityLifecycleActivity"
            android:label="@string/app_name_activity_lifecycle" >
        </activity>
        
         <activity
            android:name=".ToggleButtonActivity"
            android:label="ToggleButton" >
        </activity>
        <activity
            android:name=".DateActivity"
            android:label="时间" >
        </activity>
    </application>

</manifest>